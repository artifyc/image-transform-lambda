{
    "production": {
        "events": [{
            "function": "image_resize.lambda_handler",
            "event_source": {
                "arn": "arn:aws:s3:::image_resize",
                "events": [
                    "s3:ObjectCreated:*"
                ]
            }
        }],
        "aws_region": "us-east-1",
        "profile_name": "default",
        "s3_bucket": "artifyc-user-images-qa",
        "timeout_seconds": 30
    }
}
